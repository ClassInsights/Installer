{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:8080"
      },
      "Https": {
        "Url": "https://*:8081",
        "Certificate": {
          "Path": "./cert.pfx",
          "Password": "[PFX_PASSWORD]"
        }
      }
    }
  },
  "ComputerToken": "[COMPUTER_TOKEN]",
  "ConnectionStrings": {
    "npgsql": "Host=postgres;Database=classinsights_api;Username=classinsights;Password=[POSTGRES_PASSWORD]"
  },
  "JwtKey": "[JWT_KEY]",
  "License": "[LICENSE_KEY]",
  "Server": "https://classinsights.at"
}
